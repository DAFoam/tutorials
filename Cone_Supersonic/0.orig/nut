FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }
    outlet
    {
        type            calculated;
        value           $internalField;
    }
    cone
    {
        type            boundaryCorrectedNutLowReWallFunction;
        value           uniform 1e-16;
    }
    top_bot
    {
        type            calculated;
        value           $internalField;
    }
    syms
    {
        type            symmetry;
    }
}


// ************************************************************************* //
